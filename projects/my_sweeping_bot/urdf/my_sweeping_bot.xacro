<?xml version='1.0'?>

<robot name="my_sweeping_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find my_sweeping_bot)/urdf/my_sweeping_bot.gazebo" />

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <cylinder radius="0.175" length="0.08"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <cylinder radius="0.175" length="0.08"/>
      </geometry>
    </visual>

  </link>

  <link name="castor">
    <pose>0 0 0 0 0 0</pose>

    <inertial>
        <mass value="1.0"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia
            ixx="0.0002" ixy="0" ixz="0"
            iyy="0.0002" iyz="0"
            izz="0.0002"
        />
    </inertial>

    <visual name="castor_visual">
        <geometry>
            <sphere radius="0.02"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>

    <collision name="castor_collision">
        <geometry>
            <sphere radius="0.02"/>
        </geometry>
       <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>

  </link>

  <joint type="fixed" name="castor_joint">
    <origin xyz="0.15 0.0 -0.04" rpy="0 0 0"/>
    <child link="castor"/>
    <parent link="chassis"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="fake_castor">
    <pose>0 0 0 0 0 0</pose>

    <inertial>
        <mass value="1.0"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia
            ixx="0.0002" ixy="0" ixz="0"
            iyy="0.0002" iyz="0"
            izz="0.0002"
        />
    </inertial>

    <visual name="fake_castor_visual">
        <geometry>
            <sphere radius="0.02"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>

    <collision name="fake_castor_collision">
        <geometry>
            <sphere radius="0.02"/>
        </geometry>
       <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>

  </link>

  <joint type="fixed" name="fake_castor_joint">
    <origin xyz="-0.15 0.0 -0.04" rpy="0 0 0"/>
    <child link="fake_castor"/>
    <parent link="chassis"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="left_wheel">
    <pose>0 0 0 0 0 0</pose>

    <inertial>
        <mass value="1.0"/>
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <inertia
            ixx="0.0001" ixy="0" ixz="0"
            iyy="0.0001" iyz="0"
            izz="0.0001"
        />
    </inertial>

    <visual name="left_wheel_visual">
        <geometry>
            <cylinder length="0.02" radius="0.02"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    </visual>

    <collision name="left_wheel_collision">
        <geometry>
            <cylinder length="0.02" radius="0.02"/>
        </geometry>
       <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    </collision>

  </link>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0.0 0.15 -0.04" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

 <link name="right_wheel">
    <inertial>
        <mass value="1.0"/>
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
        <inertia
            ixx="0.0001" ixy="0" ixz="0"
            iyy="0.0001" iyz="0"
            izz="0.0001"
        />
    </inertial>

    <visual name="right_wheel_visual">
        <geometry>
            <cylinder length="0.02" radius="0.02"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    </visual>

    <collision name="right_wheel_collision">
        <geometry>
            <cylinder length="0.02" radius="0.02"/>
        </geometry>
       <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    </collision>

  </link>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0.0 -0.15 -0.04" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <link name="camera">
      <inertial>
          <mass value="0.1"/>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <inertia
              ixx="1e-6" ixy="0" ixz="0" 
              iyy="1e-6" iyz="0" izz="1e-6"
          />
      </inertial>

      <collision name="camera-collision">
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
             <box size="0.05 0.05 0.05"/>
         </geometry> 
      </collision>

      <visual name="camera_visual">
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry> 
             <box size="0.05 0.05 0.05"/>
         </geometry>
      </visual>
  </link>

  <joint type="fixed" name="camera_joint">
      <origin xyz="0.2 0 0" rpy="0 0 0"/>
      <child link="camera"/>
      <parent link="chassis"/>
  </joint>

  <link name="hokuyo">
      <inertial>
          <mass value="0.1"/>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <inertia
              ixx="1e-6" ixy="0" ixz="0" 
              iyy="1e-6" iyz="0" izz="1e-6"
          />
      </inertial>

      <collision name="hokuyo-collision">
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry> 
             <box size="0.1 0.1 0.1"/>
         </geometry>
      </collision>

      <visual name="hokuyo_visual">
         <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
             <mesh filename="package://my_sweeping_bot/meshes/hokuyo.dae"/>
         </geometry>
      </visual>
  </link>

  <joint type="fixed" name="hokuyo_joint">
      <origin xyz="0.15 0 0.08" rpy="0 0 0"/>
      <child link="hokuyo"/>
      <parent link="chassis"/>
  </joint>

</robot>

